{
  "zapier_microsoft_365_integration": {
    "status": "✅ FREE - User confirmed Zapier connected to Microsoft",
    "migration_workflows": [
      {
        "zap_id": 1,
        "name": "Microsoft 365 OneDrive → Dropbox Migration",
        "trigger": {
          "app": "Microsoft OneDrive",
          "event": "New File in Folder",
          "folder": "All Files",
          "frequency": "Every 15 minutes"
        },
        "actions": [
          {
            "step": 1,
            "app": "Dropbox",
            "action": "Upload File",
            "destination": "/Microsoft365_Backup/OneDrive/",
            "settings": {
              "create_folders": true,
              "preserve_structure": true
            }
          },
          {
            "step": 2,
            "app": "Google Sheets",
            "action": "Create Spreadsheet Row",
            "spreadsheet": "Microsoft 365 Migration Log",
            "data": {
              "timestamp": "{{trigger.created_time}}",
              "file_name": "{{trigger.name}}",
              "file_size": "{{trigger.size}}",
              "source": "OneDrive",
              "destination": "Dropbox",
              "status": "Migrated"
            }
          },
          {
            "step": 3,
            "app": "Email",
            "action": "Send Email",
            "to": "terobinsonwy@gmail.com",
            "subject": "File Migrated: {{trigger.name}}",
            "body": "Successfully migrated {{trigger.name}} from OneDrive to Dropbox"
          }
        ],
        "cost": "FREE - Zapier free tier",
        "status": "Ready to activate - just turn ON the Zap"
      },
      {
        "zap_id": 2,
        "name": "Microsoft SharePoint → Dropbox Migration",
        "trigger": {
          "app": "Microsoft SharePoint",
          "event": "New File in Library",
          "library": "All Document Libraries",
          "frequency": "Every 15 minutes"
        },
        "actions": [
          {
            "step": 1,
            "app": "Dropbox",
            "action": "Upload File",
            "destination": "/Microsoft365_Backup/SharePoint/",
            "settings": {
              "create_folders": true,
              "preserve_structure": true
            }
          },
          {
            "step": 2,
            "app": "Airtable",
            "action": "Create Record",
            "base": "Migration Tracker",
            "table": "SharePoint Files",
            "fields": {
              "File Name": "{{trigger.name}}",
              "Migrated Date": "{{trigger.created_time}}",
              "Status": "Completed"
            }
          }
        ],
        "cost": "FREE",
        "status": "Ready to activate"
      },
      {
        "zap_id": 3,
        "name": "Microsoft OneNote → Dropbox Migration",
        "trigger": {
          "app": "Microsoft OneNote",
          "event": "Updated Note",
          "notebook": "All Notebooks",
          "frequency": "Every 30 minutes"
        },
        "actions": [
          {
            "step": 1,
            "app": "Dropbox",
            "action": "Create Text File",
            "destination": "/Microsoft365_Backup/OneNote/",
            "filename": "{{trigger.title}}.txt",
            "content": "{{trigger.content}}"
          },
          {
            "step": 2,
            "app": "Slack",
            "action": "Send Channel Message",
            "channel": "#migration-updates",
            "message": "OneNote migrated: {{trigger.title}}"
          }
        ],
        "cost": "FREE",
        "status": "Ready to activate"
      },
      {
        "zap_id": 4,
        "name": "Microsoft Copilot Notes → Dropbox Backup",
        "trigger": {
          "app": "Microsoft 365 (Custom)",
          "event": "New Copilot Conversation",
          "frequency": "Every hour"
        },
        "actions": [
          {
            "step": 1,
            "app": "Dropbox",
            "action": "Create Text File",
            "destination": "/Microsoft365_Backup/Copilot_Chats/",
            "filename": "copilot_{{trigger.timestamp}}.txt",
            "content": "{{trigger.conversation}}"
          },
          {
            "step": 2,
            "app": "Google Sheets",
            "action": "Create Spreadsheet Row",
            "spreadsheet": "Copilot Backup Log"
          }
        ],
        "cost": "FREE",
        "status": "Ready to activate"
      },
      {
        "zap_id": 5,
        "name": "Microsoft Research → Dropbox Archive",
        "trigger": {
          "app": "Microsoft Edge Collections",
          "event": "New Collection Item",
          "frequency": "Daily"
        },
        "actions": [
          {
            "step": 1,
            "app": "Dropbox",
            "action": "Create Text File",
            "destination": "/Microsoft365_Backup/Research/",
            "content": "{{trigger.url}} - {{trigger.notes}}"
          }
        ],
        "cost": "FREE",
        "status": "Ready to activate"
      }
    ],
    "why_zapier_is_perfect": {
      "microsoft_connection": "Already connected to Microsoft 365",
