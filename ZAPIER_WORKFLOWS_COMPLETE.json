    {
      "name": "Financial Report Generation",
      "description": "Generate and send weekly financial reports",
      "trigger": {
        "app": "Schedule by Zapier",
        "event": "Every Week",
        "day": "Monday",
        "time": "9:00 AM CST"
      },
      "actions": [
        {
          "app": "Airtable",
          "action": "Find Records",
          "base": "Financial Operations",
          "table": "Transactions",
          "filter": "Created Time is within the last 7 days"
        },
        {
          "app": "Formatter by Zapier",
          "action": "Spreadsheet-Style Formula",
          "formula": "=SUM(Amount)",
          "note": "Calculate weekly totals"
        },
        {
          "app": "Google Docs",
          "action": "Create Document from Template",
          "template": "Weekly Financial Report",
          "replacements": {
            "{{week}}": "{{week_of_year}}",
            "{{revenue}}": "{{total_revenue}}",
            "{{expenses}}": "{{total_expenses}}",
            "{{net}}": "{{net_income}}"
          }
        },
        {
          "app": "PDF.co",
          "action": "Convert to PDF"
        },
        {
          "app": "Email",
          "action": "Send Email",
          "to": "terobinsonwy@gmail.com",
          "subject": "Weekly Financial Report - Week {{week}}",
          "body": "Attached is your weekly financial report.\n\nRevenue: {{total_revenue}}\nExpenses: {{total_expenses}}\nNet Income: {{net_income}}",
          "attachments": "{{pdf_file}}"
        }
      ]
    },
    {
      "name": "Life Coach Session Reminders",
      "description": "Send reminders for coaching sessions",
      "trigger": {
        "app": "Airtable",
        "event": "New Record",
        "base": "Life Coach AI",
        "table": "Sessions",
        "view": "Upcoming Sessions"
      },
      "actions": [
        {
          "app": "Delay by Zapier",
          "action": "Delay Until",
          "delay_until": "{{session_datetime}} - 1 day"
        },
        {
          "app": "Email",
          "action": "Send Email",
          "to": "{{client_email}}",
          "subject": "Coaching Session Reminder - Tomorrow",
          "body": "Hi {{client_name}},\n\nThis is a reminder that we have a coaching session scheduled for tomorrow.\n\nDate: {{session_date}}\nTime: {{session_time}}\nTopic: {{session_topic}}\n\nLooking forward to our session!\n\nBest,\nThurman Robinson\nCertified Life Coach"
        },
        {
          "app": "Slack",
          "action": "Send Direct Message",
          "user": "Thurman Robinson",
          "message": "ðŸ“… Session reminder: {{client_name}} tomorrow at {{session_time}}"
        }
      ]
    },
    {
      "name": "Continuous Test Runner (24/7)",
      "description": "Run automated tests every hour",
      "trigger": {
        "app": "Schedule by Zapier",
        "event": "Every Hour"
      },
      "actions": [
        {
          "app": "Webhooks by Zapier",
          "action": "POST",
          "url": "https://api.github.com/repos/appsefilepro-cell/Private-Claude/dispatches",
          "method": "POST",
          "headers": {
            "Authorization": "Bearer {{github_token}}",
            "Accept": "application/vnd.github.v3+json"
          },
          "data": {
            "event_type": "run_tests",
            "client_payload": {
              "test_suite": "all",
              "parallel": true
            }
          }
        },
        {
          "app": "Slack",
          "action": "Send Channel Message",
          "channel": "#test-results",
          "message": "ðŸ”„ Hourly test run initiated"
        }
      ]
    }
  ],
  "setup_instructions": {
