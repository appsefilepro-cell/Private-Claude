{
  "workflows": [
    {
      "name": "GitHub to GitLab Sync",
      "description": "Automatically sync GitHub commits to GitLab",
      "trigger": {
        "app": "GitHub",
        "event": "New Commit",
        "account": "appsefilepro",
        "repository": "Private-Claude"
      },
      "actions": [
        {
          "app": "GitLab",
          "action": "Mirror Commit",
          "project": "appsefilepro-project"
        },
        {
          "app": "Slack",
          "action": "Send Channel Message",
          "channel": "#system-status",
          "message": "âœ… GitHub commit synced to GitLab: {{commit_message}}"
        },
        {
          "app": "Email",
          "action": "Send Email",
          "to": "terobinsonwy@gmail.com",
          "subject": "GitHub Sync Complete",
          "body": "Commit synced: {{commit_message}}"
        }
      ]
    },
    {
      "name": "GitLab to GitHub Sync",
      "description": "Automatically sync GitLab pushes to GitHub",
      "trigger": {
        "app": "GitLab",
        "event": "New Push",
        "project": "appsefilepro-project"
      },
      "actions": [
        {
          "app": "GitHub",
          "action": "Create Commit",
          "repository": "Private-Claude"
        },
        {
          "app": "Slack",
          "action": "Send Channel Message",
          "channel": "#system-status",
          "message": "âœ… GitLab push synced to GitHub"
        }
      ]
    },
    {
      "name": "OKX Trade Notifications",
      "description": "Send email/Slack alerts for every OKX trade",
      "trigger": {
        "app": "Webhooks by Zapier",
        "event": "Catch Hook",
        "webhook_url": "https://hooks.zapier.com/hooks/catch/YOUR_WEBHOOK_ID/",
        "note": "Configure OKX bot to send trade data to this webhook"
      },
      "actions": [
        {
          "app": "Google Sheets",
          "action": "Create Spreadsheet Row",
          "spreadsheet": "OKX Trading Log",
          "worksheet": "Trades",
          "data": {
            "Timestamp": "{{timestamp}}",
            "Pair": "{{pair}}",
            "Side": "{{side}}",
            "Amount": "{{amount}}",
            "Price": "{{price}}",
            "Pattern": "{{pattern}}",
            "P&L": "{{pnl}}"
          }
        },
        {
          "app": "Slack",
          "action": "Send Channel Message",
          "channel": "#trading-alerts",
          "message": "ðŸ”” OKX Trade: {{side}} {{amount}} {{pair}} at {{price}} | Pattern: {{pattern}} | P&L: {{pnl}}"
        },
        {
          "app": "Email",
          "action": "Send Email",
          "to": "terobinsonwy@gmail.com",
          "subject": "OKX Trade Alert: {{pair}}",
          "body": "Trade executed:\nPair: {{pair}}\nSide: {{side}}\nAmount: {{amount}}\nPrice: {{price}}\nPattern: {{pattern}}\nP&L: {{pnl}}"
        }
      ]
    },
    {
      "name": "Daily Trading Report",
      "description": "Send daily P&L report from OKX trading",
      "trigger": {
        "app": "Schedule by Zapier",
        "event": "Every Day",
        "time": "8:00 PM CST"
      },
      "actions": [
        {
          "app": "Google Sheets",
          "action": "Lookup Spreadsheet Rows",
          "spreadsheet": "OKX Trading Log",
          "worksheet": "Trades",
          "search_column": "Timestamp",
          "search_value": "{{today}}"
        },
        {
          "app": "Formatter by Zapier",
          "action": "Spreadsheet-Style Formula",
          "formula": "=SUM(P&L)",
          "note": "Calculate total daily P&L"
        },
        {
          "app": "Email",
          "action": "Send Email",
          "to": "terobinsonwy@gmail.com",
          "subject": "Daily OKX Trading Report - {{today}}",
          "body": "Daily Trading Summary:\n\nTotal Trades: {{count}}\nTotal P&L: {{total_pnl}}\nWin Rate: {{win_rate}}%\nBest Trade: {{best_trade}}\nWorst Trade: {{worst_trade}}\n\nView full log: [Google Sheets Link]"
        },
        {
          "app": "Slack",
          "action": "Send Channel Message",
          "channel": "#trading-alerts",
          "message": "ðŸ“Š Daily Report: {{count}} trades | P&L: {{total_pnl}} | Win Rate: {{win_rate}}%"
        }
      ]
    },
    {
      "name": "Legal Document Generated",
      "description": "Notify when legal document is generated",
      "trigger": {
        "app": "Google Drive",
        "event": "New File in Folder",
        "folder": "Legal Automation/Generated Documents"
      },
      "actions": [
        {
          "app": "PDF.co",
          "action": "Convert to PDF",
          "note": "Ensure document is in PDF format"
        },
        {
          "app": "Email",
          "action": "Send Email",
          "to": "terobinsonwy@gmail.com",
          "subject": "Legal Document Ready: {{file_name}}",
          "body": "Your legal document has been generated and is ready for review.\n\nDocument: {{file_name}}\nGenerated: {{timestamp}}\n\nView document: {{file_link}}",
          "attachments": "{{pdf_file}}"
        },
        {
          "app": "Slack",
          "action": "Send Channel Message",
          "channel": "#legal-updates",
          "message": "ðŸ“„ Legal document ready: {{file_name}}\nLink: {{file_link}}"
        }
      ]
    },
    {
      "name": "Case 1241511 Dismissal Workflow",
      "description": "Automate probate case dismissal notice generation and sending",
      "trigger": {
        "app": "Webhooks by Zapier",
        "event": "Catch Hook",
        "webhook_url": "https://hooks.zapier.com/hooks/catch/YOUR_WEBHOOK_ID/case1241511",
        "note": "Trigger this manually or via button"
      },
      "actions": [
        {
          "app": "Google Docs",
          "action": "Create Document from Template",
          "template": "Probate Dismissal Notice Template",
          "replacements": {
            "{{case_number}}": "1241511",
            "{{court}}": "Harris County - County Civil Court at Law No. 2",
            "{{plaintiff}}": "NEW FOREST HOUSTON 2020 LLC",
            "{{defendant}}": "THURMAN ROBINSON, ET AL.",
            "{{date}}": "{{today}}"
          }
        },
        {
          "app": "PDF.co",
          "action": "Convert Doc to PDF"
        },
        {
          "app": "Email",
          "action": "Send Email",
          "to": "terobinsonwy@gmail.com",
          "subject": "Case 1241511 - Dismissal Notice Generated",
          "body": "The dismissal notice for Case No. 1241511 has been generated.\n\nPlease review the attached document before filing with the court.",
          "attachments": "{{pdf_file}}"
        },
        {
          "app": "Airtable",
          "action": "Create Record",
          "base": "Legal Cases",
          "table": "Documents",
          "fields": {
            "Case Number": "1241511",
            "Document Type": "Dismissal Notice",
            "Generated Date": "{{today}}",
            "Status": "Ready for Filing",
            "File Link": "{{file_link}}"
          }
        }
      ]
    },
    {
      "name": "Form 1023-EZ Nonprofit Application",
      "description": "Automate IRS Form 1023-EZ filling",
      "trigger": {
        "app": "Webhooks by Zapier",
        "event": "Catch Hook",
        "webhook_url": "https://hooks.zapier.com/hooks/catch/YOUR_WEBHOOK_ID/form1023",
        "data_source": "Airtable nonprofit data"
      },
      "actions": [
        {
          "app": "Airtable",
          "action": "Find Record",
          "base": "Nonprofit Operations",
          "table": "Organization Info",
          "search_field": "Organization Name",
          "search_value": "{{org_name}}"
        },
        {
          "app": "PDF.co",
          "action": "Fill PDF Form",
          "template_url": "https://www.irs.gov/pub/irs-pdf/f1023ez.pdf",
          "fields": {
            "OrganizationName": "{{org_name}}",
            "EIN": "{{ein}}",
            "Address": "{{address}}",
            "City": "{{city}}",
            "State": "{{state}}",
            "ZIP": "{{zip}}",
            "FormationDate": "{{formation_date}}",
            "Purpose": "{{purpose}}"
          }
        },
        {
          "app": "Google Drive",
          "action": "Upload File",
          "folder": "Legal Automation/Nonprofit Forms",
          "file_name": "Form_1023_EZ_{{org_name}}.pdf"
        },
        {
          "app": "Email",
          "action": "Send Email",
          "to": "terobinsonwy@gmail.com",
          "subject": "Form 1023-EZ Ready: {{org_name}}",
          "body": "Form 1023-EZ has been completed for {{org_name}}.\n\nNext steps:\n1. Review the form\n2. Pay the filing fee ($275)\n3. Submit to IRS via Pay.gov\n\nView form: {{file_link}}",
          "attachments": "{{pdf_file}}"
        }
      ]
    },
    {
      "name": "Microsoft 365 to Google Drive Migration",
      "description": "Automate document migration from M365 to Google Drive",
      "trigger": {
        "app": "OneDrive",
        "event": "New File",
        "folder": "/",
        "note": "Triggers for each file in OneDrive"
      },
      "actions": [
        {
          "app": "Google Drive",
          "action": "Upload File",
          "folder": "Migrated from Microsoft 365",
          "file": "{{file}}",
          "preserve_metadata": true
        },
        {
          "app": "Airtable",
          "action": "Create Record",
          "base": "Migration Tracking",
          "table": "Migrated Files",
          "fields": {
            "File Name": "{{file_name}}",
            "Original Location": "OneDrive",
            "New Location": "Google Drive",
            "Migrated Date": "{{today}}",
            "File Size": "{{file_size}}",
            "Link": "{{gdrive_link}}"
          }
        },
        {
          "app": "Formatter by Zapier",
          "action": "Numbers",
          "operation": "Perform Math Operation",
          "note": "Track total migrated size"
        }
      ]
    },
    {
      "name": "Slack AI Communication Hub",
      "description": "Enable AI-to-AI communication via Slack",
      "trigger": {
        "app": "Slack",
        "event": "New Message Posted to Channel",
        "channel": "#ai-communication"
      },
      "actions": [
        {
          "app": "Formatter by Zapier",
          "action": "Text",
          "operation": "Extract Pattern",
          "pattern": "@Agent5CFO|@Committee100",
          "note": "Parse AI mentions"
        },
        {
          "app": "Webhooks by Zapier",
          "action": "POST",
          "url": "https://api.anthropic.com/v1/messages",
          "note": "Send to Claude API for processing"
        },
        {
          "app": "Slack",
          "action": "Send Channel Message",
          "channel": "#ai-communication",
          "message": "{{ai_response}}"
        }
      ]
    },
    {
      "name": "System Error Alerts",
      "description": "Alert on any system errors",
      "trigger": {
        "app": "Webhooks by Zapier",
        "event": "Catch Hook",
        "webhook_url": "https://hooks.zapier.com/hooks/catch/YOUR_WEBHOOK_ID/errors"
      },
      "actions": [
        {
          "app": "Airtable",
          "action": "Create Record",
          "base": "System Monitoring",
          "table": "Errors",
          "fields": {
            "Timestamp": "{{timestamp}}",
            "System": "{{system}}",
            "Error": "{{error_message}}",
            "Severity": "{{severity}}",
            "Status": "New"
          }
        },
        {
          "app": "Slack",
          "action": "Send Channel Message",
          "channel": "#system-status",
          "message": "ðŸš¨ ERROR in {{system}}: {{error_message}}",
          "priority": "urgent"
        },
        {
          "app": "Email",
          "action": "Send Email",
          "to": "terobinsonwy@gmail.com",
          "subject": "URGENT: System Error in {{system}}",
          "body": "An error has occurred:\n\nSystem: {{system}}\nError: {{error_message}}\nTime: {{timestamp}}\nSeverity: {{severity}}\n\nView in Airtable: {{record_link}}"
        }
      ]
    },
    {
      "name": "Test Completion Notifications",
      "description": "Notify when tests complete",
      "trigger": {
