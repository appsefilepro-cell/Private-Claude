        },
        {
          "app": "Email",
          "action": "Send Email",
          "to": "terobinsonwy@gmail.com",
          "subject": "Test Results: {{test_name}}",
          "body": "Test: {{test_name}}\nStatus: Complete\nPass Rate: {{pass_rate}}%\nResults: {{results}}\n\nView full report: {{report_link}}"
        }
      ]
    },
    {
      "name": "E2B Sandbox Execution",
