          "name": "ML Pattern Analysis on Winning Trades",
          "zap_id": "ZAP-009-TRADING-ML",
          "trigger": {
            "app": "Google Sheets",
            "event": "New Row",
            "spreadsheet": "Trading Log - MT5",
