        "workflow_4": {
          "name": "Outlook Emails to Dropbox Backup",
          "zap_id": "ZAP-004-MIGRATION-OUTLOOK",
          "trigger": {
            "app": "Microsoft Outlook",
            "event": "New Email",
            "folder": "Sent Items",
            "frequency": "Every 5 minutes"
          },
          "actions": [
            {
              "step": 1,
              "app": "Filter by Zapier",
              "action": "Only continue if has attachments OR important"
            },
            {
              "step": 2,
              "app": "Formatter by Zapier",
              "action": "Date/Time",
              "transform": "Format {{trigger.sent_time}} to YYYY-MM-DD"
            },
            {
              "step": 3,
              "app": "Dropbox",
              "action": "Create Text File",
              "folder": "/Microsoft365_Backup/Outlook/{{formatted_date}}/",
              "filename": "{{trigger.subject}}-{{trigger.timestamp}}.txt",
              "content": "From: {{trigger.from}}\nTo: {{trigger.to}}\nSubject: {{trigger.subject}}\nDate: {{trigger.sent_time}}\n\n{{trigger.body}}"
            },
            {
              "step": 4,
              "app": "Paths by Zapier",
              "paths": [
                {
                  "name": "Has Attachments",
                  "condition": "Attachments count > 0",
                  "actions": [
                    {
                      "app": "Looping by Zapier",
                      "action": "Loop through attachments",
                      "sub_actions": [
                        {
                          "app": "Dropbox",
                          "action": "Upload File",
                          "folder": "/Microsoft365_Backup/Outlook/Attachments/"
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "step": 5,
              "app": "Google Sheets",
              "action": "Create Spreadsheet Row",
              "spreadsheet": "Migration Log - Outlook Emails"
            }
          ],
          "cost": "FREE",
          "status": "READY TO ACTIVATE"
        },
