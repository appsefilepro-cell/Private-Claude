            {
              "step": 2,
              "app": "Code by Zapier",
              "language": "Python",
              "code": "# Analyze pattern frequency\nimport json\npatterns = input_data.get('patterns', []).split(',')\npattern_count = {}\nfor p in patterns:\n    pattern_count[p] = pattern_count.get(p, 0) + 1\nbest_pattern = max(pattern_count, key=pattern_count.get)\noutput = {'best_pattern': best_pattern, 'frequency': pattern_count[best_pattern]}"
            },
            {
              "step": 3,
              "app": "Airtable",
              "action": "Create Record",
              "base": "Trading Intelligence",
              "table": "Winning Patterns",
