          },
          "actions": [
            {
              "step": 1,
              "app": "Formatter by Zapier",
              "action": "Numbers",
              "operation": "Perform Math Operation",
              "formula": "{{trade.volume}} * {{trade.price}}",
              "output": "trade_value"
            },
            {
              "step": 2,
              "app": "Google Sheets",
              "action": "Create Spreadsheet Row",
              "spreadsheet": "Trading Log - MT5",
              "worksheet": "All Trades",
              "values": {
                "Timestamp": "{{trigger.timestamp}}",
                "Account": "{{trigger.account}}",
                "Symbol": "{{trigger.symbol}}",
                "Action": "{{trigger.action}}",
                "Volume": "{{trigger.volume}}",
                "Price": "{{trigger.price}}",
                "Trade Value": "{{trade_value}}",
                "Stop Loss": "{{trigger.sl}}",
                "Take Profit": "{{trigger.tp}}",
