    "workflows": {
      "migration_workflows": {
        "workflow_1": {
          "name": "OneDrive to Dropbox Complete Migration",
          "zap_id": "ZAP-001-MIGRATION-ONEDRIVE",
          "trigger": {
            "app": "Microsoft OneDrive",
            "event": "New File in Folder",
            "folder": "All folders",
            "frequency": "Every 15 minutes"
          },
          "actions": [
            {
              "step": 1,
              "app": "Filter by Zapier",
              "action": "Only continue if file size > 0 bytes"
            },
            {
              "step": 2,
              "app": "Dropbox",
              "action": "Upload File",
              "folder": "/Microsoft365_Backup/OneDrive/{{file_path}}",
              "create_folders": true
            },
            {
              "step": 3,
              "app": "Google Sheets",
              "action": "Create Spreadsheet Row",
              "spreadsheet": "Migration Log - OneDrive",
              "worksheet": "Files Migrated",
              "values": {
                "Timestamp": "{{trigger.created_time}}",
                "File Name": "{{trigger.name}}",
                "File Size": "{{trigger.size}}",
                "Source Path": "{{trigger.path}}",
                "Destination": "Dropbox/Microsoft365_Backup/OneDrive/",
                "Status": "Migrated Successfully"
              }
            },
            {
              "step": 4,
              "app": "Airtable",
              "action": "Create Record",
              "base": "Migration Tracker",
              "table": "OneDrive Files",
              "fields": {
                "File Name": "{{trigger.name}}",
                "Migration Date": "{{trigger.created_time}}",
                "File Size (bytes)": "{{trigger.size}}",
                "Status": "Completed"
              }
            },
            {
              "step": 5,
              "app": "Slack",
              "action": "Send Channel Message",
              "channel": "#migration-updates",
              "message": "âœ… OneDrive file migrated: {{trigger.name}} ({{trigger.size}} bytes)",
              "username": "Migration Bot"
            },
            {
              "step": 6,
              "app": "Email by Zapier",
              "action": "Send Outbound Email",
              "to": "terobinsonwy@gmail.com",
              "subject": "OneDrive Migration: {{trigger.name}}",
              "body": "File successfully migrated from OneDrive to Dropbox.\n\nDetails:\n- File: {{trigger.name}}\n- Size: {{trigger.size}}\n- Time: {{trigger.created_time}}\n- Destination: Dropbox/Microsoft365_Backup/OneDrive/",
              "send_only_on_error": false
            }
          ],
          "cost": "FREE",
          "status": "READY TO ACTIVATE"
        },
