        {
          "step": 2,
          "app": "Formatter by Zapier",
          "action": "Numbers",
          "operation": "Spreadsheet-Style Formula",
          "formula": "=AVERAGE(P&L)",
          "note": "Calculate average profit for pattern"
        },
        {
          "step": 3,
          "app": "Formatter by Zapier",
          "action": "Numbers",
          "operation": "Perform Math Operation",
          "expression": "({{winning_trades}} / {{total_trades}}) * 100",
          "note": "Calculate win rate for pattern"
        },
        {
          "step": 4,
          "app": "Airtable",
          "action": "Update Record",
          "base": "Trading Machine Learning",
          "table": "Strategy Insights",
          "fields": {
            "Pattern": "{{pattern}}",
            "Win Rate": "{{win_rate}}",
            "Avg Profit": "{{avg_profit}}",
            "Total Trades": "{{total_trades}}",
            "Last Updated": "{{timestamp}}"
