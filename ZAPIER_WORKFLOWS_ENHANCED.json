      "id": 5,
      "name": "Machine Learning - Pattern Detection",
      "description": "Analyze winning trades and detect patterns",
      "trigger": {
        "app": "Airtable",
        "event": "New Record",
        "base": "Trading Machine Learning",
        "table": "Trades",
        "view": "Winning Trades Only"
