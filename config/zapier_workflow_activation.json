  "zapier_settings": {
    "plan": "Free (100 tasks/month)",
    "optimization": {
      "use_filters": true,
      "batch_processing": true,
      "delay_non_urgent": true,
      "deduplicate": true
    },
    "monitoring": {
      "track_task_usage": true,
      "alert_at_80_percent": true,
      "weekly_summary": true
    }
  }
}
