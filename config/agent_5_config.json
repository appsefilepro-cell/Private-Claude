    "failure_recovery": true,
    "rollback_on_critical_error": true,
    "health_check_enabled": true
  },

  "pillar_a_trading": {
    "enabled": true,
    "name": "Automated Trading Automation",
    "exchanges": {
      "kraken": {
        "enabled": true,
        "api_endpoint": "https://api.kraken.com",
        "ws_endpoint": "wss://ws.kraken.com",
        "supported_pairs": ["BTC/USD", "ETH/USD", "SOL/USD", "XRP/USD"],
        "trading_mode": "sandbox",
        "max_concurrent_trades": 5,
        "position_sizing": "dynamic"
      },
      "metatrader5": {
        "enabled": true,
        "platform": "MT5",
        "supported_symbols": ["EURUSD", "GBPUSD", "USDJPY", "AUDUSD"],
        "trading_mode": "sandbox",
        "max_concurrent_orders": 10,
        "timeframes": ["M1", "M5", "M15", "H1", "D1"]
      }
    },
    "risk_management": {
      "confidence_threshold": 0.75,
      "max_position_size": 0.02,
      "risk_per_trade": 0.01,
      "max_daily_loss_percent": 5,
      "max_weekly_loss_percent": 10,
      "max_open_positions": 5,
      "emergency_halt_threshold": 15,
      "trailing_stop_enabled": true,
      "trailing_stop_percent": 2.5
    },
    "candlestick_patterns": {
      "bullish": [
        "HAMMER",
        "INVERTED_HAMMER",
        "BULLISH_ENGULFING",
        "MORNING_STAR",
        "THREE_WHITE_SOLDIERS",
        "DRAGONFLY_DOJI",
        "PIERCING_LINE"
      ],
      "bearish": [
        "SHOOTING_STAR",
        "HANGING_MAN",
        "BEARISH_ENGULFING",
        "EVENING_STAR",
        "THREE_BLACK_CROWS",
        "GRAVESTONE_DOJI",
        "DARK_CLOUD_COVER"
      ]
    },
    "indicators": {
      "moving_averages": ["SMA_20", "SMA_50", "SMA_200", "EMA_12", "EMA_26"],
      "oscillators": ["RSI", "MACD", "Stochastic"],
      "volatility": ["Bollinger_Bands", "ATR"]
    },
    "monitoring": {
      "check_interval_seconds": 60,
      "health_check_interval": 300,
      "trade_execution_timeout": 30,
      "market_data_lag_alert": 5000
    }
  },

  "pillar_b_legal": {
    "enabled": true,
