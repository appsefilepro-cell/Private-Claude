{
  "zapier_integration": {
    "name": "Agent X5 - Zapier Automation Hub",
    "version": "5.0.0",
    "description": "Full automation integration - triggers GitHub Actions, E2B sandbox, and 219 agents"
  },

  "github_actions_triggers": {
    "repository_dispatch_url": "https://api.github.com/repos/appsefilepro-cell/Private-Claude/dispatches",
    "method": "POST",
    "required_headers": {
      "Authorization": "Bearer ${GITHUB_TOKEN}",
      "Accept": "application/vnd.github.v3+json",
      "Content-Type": "application/json"
    },
    "event_types": [
      "trading-signal",
      "bonds-alert",
      "api-health",
      "issue-created",
      "zapier-trigger",
      "external-automation"
    ]
  },

  "scheduled_automations": {
    "bonds_trading": {
      "schedule": "Every hour",
      "cron": "0 * * * *",
      "event_type": "bonds-alert",
      "agents": 30,
      "division": "Trading"
    },
    "trading_dashboard": {
      "schedule": "Every 6 hours",
      "cron": "0 */6 * * *",
      "event_type": "trading-signal",
      "agents": 30,
      "division": "Trading"
    },
    "api_health_check": {
      "schedule": "Daily at 6 AM UTC",
      "cron": "0 6 * * *",
      "event_type": "api-health",
      "agents": 12,
      "division": "DevOps"
    },
    "issue_monitoring": {
      "schedule": "Every 15 minutes",
      "cron": "*/15 * * * *",
      "event_type": "issue-created",
      "agents": 219,
      "division": "All"
    }
  },

  "triggers": {
    "new_execution": {
      "event": "execution.completed",
      "description": "Triggers when E2B completes a code execution",
      "output_fields": [
        {"key": "execution_id", "label": "Execution ID"},
        {"key": "status", "label": "Status"},
        {"key": "output", "label": "Output"},
        {"key": "timestamp", "label": "Timestamp"},
        {"key": "sandbox_id", "label": "Sandbox ID"}
      ]
    },
    "file_created": {
      "event": "file.uploaded",
      "description": "Triggers when a file is uploaded to E2B",
      "output_fields": [
        {"key": "file_name", "label": "File Name"},
        {"key": "file_url", "label": "File URL"},
        {"key": "file_size", "label": "File Size"},
        {"key": "mime_type", "label": "MIME Type"}
      ]
    },
    "github_workflow_complete": {
      "event": "workflow.completed",
      "description": "Triggers when GitHub Actions workflow completes",
      "output_fields": [
        {"key": "workflow_name", "label": "Workflow Name"},
        {"key": "status", "label": "Status"},
        {"key": "run_id", "label": "Run ID"},
        {"key": "conclusion", "label": "Conclusion"}
      ]
    }
  },

  "actions": {
    "execute_code": {
      "description": "Execute code in E2B sandbox",
      "input_fields": [
        {"key": "code", "label": "Code to Execute", "required": true},
        {"key": "language", "label": "Language", "default": "python"},
        {"key": "timeout", "label": "Timeout (seconds)", "default": 30}
      ]
    },
    "upload_file": {
      "description": "Upload file to E2B sandbox",
      "input_fields": [
        {"key": "file_url", "label": "File URL", "required": true},
        {"key": "destination_path", "label": "Destination Path"}
      ]
    }
  },

  "webhook_config": {
    "method": "POST",
    "headers": {
      "Content-Type": "application/json",
      "X-Webhook-Source": "E2B-Private-Claude"
    },
    "retry_on_fail": true,
    "max_retries": 3
  },

  "common_zap_workflows": [
    {
      "name": "E2B to Google Sheets",
      "trigger": "new_execution",
      "action": "Add row to Google Sheets"
    },
    {
      "name": "E2B to Slack",
      "trigger": "execution.completed",
      "action": "Send Slack notification"
    },
    {
      "name": "E2B to Notion",
      "trigger": "file_created",
      "action": "Create Notion database item"
    },
    {
      "name": "GitHub to E2B",
      "trigger": "GitHub push",
      "action": "Execute code in E2B"
    }
  ],

  "api_keys_storage": {
    "e2b_api_key": "${E2B_API_KEY}",
    "gemini_api_key": "${GEMINI_API_KEY}",
    "github_token": "${GITHUB_TOKEN}"
  }
}
