    "google_gemini_integration": {
      "service": "Google Gemini AI (FREE)",
      "api_endpoint": "https://generativelanguage.googleapis.com/v1/models/gemini-pro:generateContent",
      "features": [
        "Free AI chat and analysis",
        "Document processing",
        "Code generation",
        "Legal research assistance",
        "Property data analysis"
      ],
      "setup": {
        "step_1": "Go to https://makersuite.google.com/app/apikey",
        "step_2": "Create free API key",
        "step_3": "Add to config: GEMINI_API_KEY",
        "step_4": "Use for legal research and document analysis"
      },
      "use_cases": [
        "Analyze probate documents",
        "Research property titles and deeds",
        "Process unclaimed property data",
        "FTC claims research",
        "Legal document generation"
      ],
      "agent_assignment": "AI/ML Division - 5 agents delegated to Gemini integration",
      "cost": "FREE - 60 requests/minute"
    },
    "google_cli_integration": {
      "service": "Google Cloud CLI (FREE tier)",
      "features": [
        "Google Sheets API (migration logs)",
        "Gmail API (email automation)",
        "Google Drive API (document storage)",
        "Google Calendar API (scheduling)"
      ],
      "setup": {
        "install": "curl https://sdk.cloud.google.com | bash",
        "init": "gcloud init",
        "auth": "gcloud auth login",
        "apis": "Enable Sheets, Gmail, Drive APIs"
      },
      "gmail_automation": {
        "read_sent_emails": {
          "method": "GET",
          "endpoint": "https://gmail.googleapis.com/gmail/v1/users/me/messages",
          "query": "in:sent",
          "purpose": "Read and reply to sent emails automatically"
        },
        "read_documents": {
          "method": "GET",
          "endpoint": "https://gmail.googleapis.com/gmail/v1/users/me/messages/{messageId}/attachments/{attachmentId}",
          "purpose": "Download and process email attachments"
        },
        "auto_reply": {
          "method": "POST",
          "endpoint": "https://gmail.googleapis.com/gmail/v1/users/me/messages/send",
          "purpose": "Automatically reply to emails"
        }
      },
      "outlook_integration": {
        "service": "Microsoft Graph API (FREE)",
        "endpoint": "https://graph.microsoft.com/v1.0",
        "features": [
          "Read sent emails from Outlook",
          "Download email attachments",
          "Send automatic replies",
          "Access OneDrive documents"
        ],
        "endpoints": {
          "get_sent_emails": "GET /me/mailFolders/sentitems/messages",
          "get_attachments": "GET /me/messages/{messageId}/attachments",
          "send_email": "POST /me/sendMail",
          "get_onedrive_files": "GET /me/drive/root/children"
        }
      },
      "agent_assignment": "Communication Division + Integration Division - 15 agents",
      "cost": "FREE with existing Microsoft 365"
    },
    "chrome_ai_setup": {
      "service": "Chrome Built-in AI (FREE)",
      "features": [
        "On-device AI (no API needed)",
        "Text generation and summarization",
        "Translation",
        "Sentiment analysis"
      ],
      "setup": {
        "enable": "chrome://flags/#optimization-guide-on-device-model",
