    "blackbox_ai_integration": {
      "service": "Blackbox AI (FREE public data search)",
      "website": "https://www.blackbox.ai/",
      "features": [
        "Public records search",
        "Legal research database",
        "Property title lookup",
        "Deed records",
        "Probate records",
        "Unclaimed property search",
        "FTC claims database"
      ],
      "search_capabilities": {
        "property_titles": {
          "query_format": "site:recorder.countyclerk.com OR site:*.gov property title [PROPERTY ADDRESS]",
          "sources": [
            "County recorder offices",
            "State land records",
            "Public property databases"
          ]
        },
        "probate_records": {
          "query_format": "site:*.gov probate records [NAME] [COUNTY]",
          "sources": [
            "County probate courts",
            "State court records",
            "Public docket searches"
          ]
        },
        "unclaimed_property": {
          "query_format": "site:unclaimed.org OR site:*.gov/unclaimed [NAME]",
          "sources": [
            "State unclaimed property databases",
            "Treasury databases",
            "Financial institution records"
          ]
        },
        "ftc_claims": {
          "query_format": "site:ftc.gov claims [COMPANY NAME]",
          "sources": [
            "FTC.gov database",
            "Consumer complaint database",
            "Settlement records"
          ]
        }
      },
      "agent_assignment": "Legal Division - 10 agents + Research Team",
      "cost": "FREE - public data access"
    },
